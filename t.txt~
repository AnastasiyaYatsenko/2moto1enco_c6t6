#!/bin/sh
echo "
2024/05/14 22:31:23 DELTA : 0.001 0.000 0.016 0.072 0.031 0.028
2024/05/14 22:31:18 DELTA : 0.010 0.030 0.135 0.083 0.006 0.165
2024/05/14 22:31:16 DELTA : 0.037 0.025 0.121 0.070 0.005 0.003
2024/05/14 22:31:14 DELTA : 0.016 0.009 0.127 0.015 0.016 0.023
2024/05/14 22:31:09 DELTA : 0.006 0.063 0.026 0.001 0.006 0.034
2024/05/14 22:31:06 DELTA : 0.068 0.554 0.026 0.114 0.011 0.011
2024/05/14 22:31:04 DELTA : 0.016 0.409 0.016 0.089 0.001 0.168
2024/05/14 22:30:59 DELTA : 0.054 0.112 0.009 0.026 0.010 0.016
2024/05/14 22:30:57 DELTA : 0.011 0.084 0.005 0.032 0.026 0.032
2024/05/14 22:30:54 DELTA : 0.017 0.000 0.016 0.016 0.016 0.016
" | LC_ALL=C awk '
BEGIN {
  for (i=0;i<=5; i++) { max[i]=0; min[i]=10 }
}
/DELTA/ { 
  f[0]=$5; f[1]=$6; f[2]=$7; f[3]=$8; f[4]=$9; f[5]=$10;
  printf "CUR  %6.4f %6.4f %6.4f %6.4f %6.4f %6.4f\n", f[0], f[1], f[2], f[3], f[4], f[5]; 
  for (i=0;i<=5; i++) { 
    a[i]+=f[i];
    if (min[i]>f[i]) { min[i]=f[i] }
    if (max[i]<f[i]) { max[i]=f[i] }
  }
}
END {
  printf "-----------------------------------------\n";
  printf "MEAN %6.4f %6.4f %6.4f %6.4f %6.4f %6.4f\n", a[0]/10, a[1]/10, a[2]/10, a[3]/10, a[4]/10, a[5]/10; 
  printf "MAX  %6.4f %6.4f %6.4f %6.4f %6.4f %6.4f\n", max[0], max[1], max[2], max[3], max[4], max[5]; 
  printf "MIN  %6.4f %6.4f %6.4f %6.4f %6.4f %6.4f\n", min[0], min[1], min[2], min[3], min[4], min[5];
}'
